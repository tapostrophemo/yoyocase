ExpiresActive On
ExpiresDefault A0
# 2 months (60*60*24*60)
ExpiresByType image/ico A5184000
ExpiresByType image/x-icon A5184000
# 1 month (60*60*24*30)
ExpiresByType image/gif A2592000
ExpiresByType image/png A2592000
ExpiresByType image/jpg A2592000
ExpiresByType image/jpeg A2592000
# 1 day (60*60*24*1)
ExpiresByType text/css A86400
ExpiresByType text/javascript A86400

FileETag none

# TODO: configure for PROD deploy
RewriteEngine on
RewriteBase /
#RewriteCond $1 !^(index\.php|favicon\.ico|res)
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
# added the following on the prod server...what did they change (and how long ago)? (3/6/2013)
#RewriteRule ^(.*)$ index.php/?$1 [L]
RewriteRule ^(.*)$ index.php/$1 [L]


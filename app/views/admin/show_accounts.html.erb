<h2>Accounts</h2>

<table width="100%" border="0" cellspacing="0" cellpadding="0" class="report">
 <tr>
  <th>Username</th>
  <th>Registered on</th>
  <th>Last login</th>
 </tr>
<% @users.each do |account| %>
 <tr>
  <td><%= account.username %></td>
  <td><%= account.created_at %></td>
  <td><%= account.last_login_at %></td>
 </tr>
<% end %>
</table>

